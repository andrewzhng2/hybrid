{
  "name": "hybrid",
  "private": true,
  "scripts": {
    "dev:backend": "cd backend && source .venv/bin/activate && uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd frontend && npm run dev",
    "dev": "npm-run-all -p dev:backend dev:frontend"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "uvicorn": "^0.0.1-security"
  }
}
